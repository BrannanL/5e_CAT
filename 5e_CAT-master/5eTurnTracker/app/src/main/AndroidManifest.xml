        <activity android:name=".MonkSubclass" />
        <activity android:name=".FighterSubclass" />
        <activity android:name=".DruidSubclass" />
        <activity android:name=".ClericSubclass" />
        <activity android:name=".BardSubclass" />
        <activity android:name=".BarbarianSubclass" />
        <activity android:name=".SpellSelect" />
        <activity android:name=".FeatSelect" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SubclassSelect" />
        <activity android:name=".raceSelect">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>